{"ast":null,"code":"import MtCard from \"@/components/MtCard.vue\";\nimport MtIconBtn from \"./MtIconBtn.vue\";\nimport MtMapDialog from \"@/dialog/MtMapDialog.vue\";\nexport default {\n  components: {\n    MtCard,\n    MtIconBtn,\n    MtMapDialog\n  },\n  props: {\n    schedule: {\n      type: Object,\n      required: true\n    },\n    timeLineHeight: {\n      type: [Number, String],\n      required: true\n    },\n    timeLineWidth: {\n      type: [Number, String],\n      required: true\n    }\n  },\n  data: () => ({\n    isShowTipsIcon: false\n  }),\n  mounted: function () {\n    //\n  },\n  computed: {\n    // カード単体の幅\n    timeLineItemWidth: function () {\n      const timeLineLeftmargin = 100; // タイムラインの左にあるアイコンとかの幅を引く\n      return this.timeLineWidth - timeLineLeftmargin;\n    }\n  },\n  methods: {\n    onClick: function (actionName, item) {\n      if (actionName == \"homePage\") window.open(item.homepage);\n      if (actionName == \"map\") this.$refs.MapDialog.show(\"Google Map\", item.map);\n      if (actionName == \"tips\") this.$refs.MoreDialog.show(\"Tips\", item.more);\n    }\n  }\n};","map":{"version":3,"names":["MtCard","MtIconBtn","MtMapDialog","components","props","schedule","type","Object","required","timeLineHeight","Number","String","timeLineWidth","data","isShowTipsIcon","mounted","computed","timeLineItemWidth","timeLineLeftmargin","methods","onClick","actionName","item","window","open","homepage","$refs","MapDialog","show","map","MoreDialog","more"],"sources":["C:\\work\\my-trip\\src\\components\\MtTimeLine.vue"],"sourcesContent":["<!-- MtTimeLine.vue -->\r\n<template>\r\n  <MtCard\r\n    flat\r\n    style=\"overflow: auto; padding-left: 16px\"\r\n    :height=\"timeLineHeight\"\r\n    :width=\"timeLineWidth\"\r\n  >\r\n    <v-timeline\r\n      style=\"margin-bottom: 20px\"\r\n      density=\"compact\"\r\n      align=\"start\"\r\n      side=\"end\"\r\n    >\r\n      <v-timeline-item\r\n        v-for=\"(item, index) in schedule\"\r\n        :key=\"index\"\r\n        :dot-color=\"item.color\"\r\n        :icon=\"item.icon\"\r\n        fill-dot\r\n      >\r\n        <v-card :width=\"timeLineItemWidth\">\r\n          <v-card-title :class=\"['text-h6', `bg-${item.color}`]\">\r\n            {{ item.title }}\r\n          </v-card-title>\r\n          <v-card-subtitle :class=\"['text-caption', `bg-${item.color}`]\">\r\n            {{ item.subtitle }}</v-card-subtitle\r\n          >\r\n          <v-card-text class=\"text-left\">\r\n            {{ item.comment }}\r\n          </v-card-text>\r\n          <div class=\"d-flex justify-start\">\r\n            <MtIconBtn\r\n              :color=\"item.color\"\r\n              icon\r\n              height=\"30\"\r\n              width=\"30\"\r\n              icon-color=\"#ffffff\"\r\n              iconName=\"mdi-home\"\r\n              style=\"margin: 0px 4px 4px 4px\"\r\n              @click=\"onClick('homePage', item)\"\r\n            >\r\n            </MtIconBtn>\r\n            <MtIconBtn\r\n              :color=\"item.color\"\r\n              icon\r\n              height=\"30\"\r\n              width=\"30\"\r\n              icon-color=\"#ffffff\"\r\n              iconName=\"mdi-map-marker\"\r\n              style=\"margin: 0px 4px 4px 4px\"\r\n              @click=\"onClick('map', item)\"\r\n            >\r\n            </MtIconBtn>\r\n            <MtIconBtn\r\n              v-if=\"isShowTipsIcon\"\r\n              :color=\"item.color\"\r\n              icon\r\n              height=\"30\"\r\n              width=\"30\"\r\n              icon-color=\"#ffffff\"\r\n              iconName=\"mdi-alert\"\r\n              style=\"margin: 0px 4px 4px 4px\"\r\n              @click=\"onClick('tips', item)\"\r\n            >\r\n            </MtIconBtn>\r\n          </div>\r\n        </v-card>\r\n      </v-timeline-item>\r\n    </v-timeline>\r\n  </MtCard>\r\n\r\n  <MtMapDialog ref=\"MapDialog\"></MtMapDialog>\r\n</template>\r\n\r\n<script>\r\nimport MtCard from \"@/components/MtCard.vue\";\r\nimport MtIconBtn from \"./MtIconBtn.vue\";\r\nimport MtMapDialog from \"@/dialog/MtMapDialog.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    MtCard,\r\n    MtIconBtn,\r\n    MtMapDialog,\r\n  },\r\n  props: {\r\n    schedule: { type: Object, required: true },\r\n    timeLineHeight: { type: [Number, String], required: true },\r\n    timeLineWidth: { type: [Number, String], required: true },\r\n  },\r\n  data: () => ({\r\n    isShowTipsIcon: false,\r\n  }),\r\n  mounted: function () {\r\n    //\r\n  },\r\n  computed: {\r\n    // カード単体の幅\r\n    timeLineItemWidth: function () {\r\n      const timeLineLeftmargin = 100; // タイムラインの左にあるアイコンとかの幅を引く\r\n      return this.timeLineWidth - timeLineLeftmargin;\r\n    },\r\n  },\r\n  methods: {\r\n    onClick: function (actionName, item) {\r\n      if (actionName == \"homePage\") window.open(item.homepage);\r\n\r\n      if (actionName == \"map\")\r\n        this.$refs.MapDialog.show(\"Google Map\", item.map);\r\n\r\n      if (actionName == \"tips\") this.$refs.MoreDialog.show(\"Tips\", item.more);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AA4EA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,WAAU,MAAO,0BAA0B;AAElD,eAAe;EACbC,UAAU,EAAE;IACVH,MAAM;IACNC,SAAS;IACTC;EACF,CAAC;EACDE,KAAK,EAAE;IACLC,QAAQ,EAAE;MAAEC,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IAC1CC,cAAc,EAAE;MAAEH,IAAI,EAAE,CAACI,MAAM,EAAEC,MAAM,CAAC;MAAEH,QAAQ,EAAE;IAAK,CAAC;IAC1DI,aAAa,EAAE;MAAEN,IAAI,EAAE,CAACI,MAAM,EAAEC,MAAM,CAAC;MAAEH,QAAQ,EAAE;IAAK;EAC1D,CAAC;EACDK,IAAI,EAAEA,CAAA,MAAO;IACXC,cAAc,EAAE;EAClB,CAAC,CAAC;EACFC,OAAO,EAAE,SAAAA,CAAA,EAAY;IACnB;EAAA,CACD;EACDC,QAAQ,EAAE;IACR;IACAC,iBAAiB,EAAE,SAAAA,CAAA,EAAY;MAC7B,MAAMC,kBAAiB,GAAI,GAAG,EAAE;MAChC,OAAO,IAAI,CAACN,aAAY,GAAIM,kBAAkB;IAChD;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,EAAE,SAAAA,CAAUC,UAAU,EAAEC,IAAI,EAAE;MACnC,IAAID,UAAS,IAAK,UAAU,EAAEE,MAAM,CAACC,IAAI,CAACF,IAAI,CAACG,QAAQ,CAAC;MAExD,IAAIJ,UAAS,IAAK,KAAK,EACrB,IAAI,CAACK,KAAK,CAACC,SAAS,CAACC,IAAI,CAAC,YA