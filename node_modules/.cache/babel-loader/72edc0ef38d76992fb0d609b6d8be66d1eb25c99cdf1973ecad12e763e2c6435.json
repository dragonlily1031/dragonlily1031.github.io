mponents: {\r\n    MtCard,\r\n  },\r\n  data: () => ({\r\n    scheduleInfo : [\r\n      {\r\n        title : \"Schedule\",\r\n        url: \"/assets/sea.png\",\r\n      },\r\n      {\r\n        title : \"Hotel\",\r\n        url: \"/assets/hotel.png\",\r\n\r\n      },\r\n      {\r\n        title : \"Emergency\",\r\n        url: \"/assets/usa.png\",\r\n      },\r\n    ],\r\n    show: false,\r\n  }),\r\n  computed : {\r\n\r\n  },\r\n  methods: {\r\n    onClick: function(event) {\r\n      alert(\"onClick\" + JSON.stringify(event));\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"mappings":"AAiCA,OAAOA,MAAK,MAAO,yBAAwB;AAE3C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAI,EAAEA,CAAA,MAAO;IACXC,YAAW,EAAI,CACb;MACEC,KAAI,EAAI,UAAU;MAClBC,GAAG,EAAE;IACP,CAAC,EACD;MACED,KAAI,EAAI,OAAO;MACfC,GAAG,EAAE;IAEP,CAAC,EACD;MACED,KAAI,EAAI,WAAW;MACnBC,GAAG,EAAE;IACP,CAAC,CACF;IACDC,IAAI,EAAE;EACR,CAAC,CAAC;EACFC,QAAO,EAAI,CAEX,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,EAAE,SAAAA,CAASC,KAAK,EAAE;MACvBC,KAAK,CAAC,SAAQ,GAAIC,IAAI,CAACC,SAAS,CAACH,KAAK,CAAC,CAAC;IAC1C;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}