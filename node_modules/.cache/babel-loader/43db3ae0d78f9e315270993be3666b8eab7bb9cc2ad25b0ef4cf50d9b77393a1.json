n    cardHeight: 0,\r\n    cardWidth: 0,\r\n  }),\r\n  mounted: function () {\r\n    this.onResize();\r\n  },\r\n  methods: {\r\n    onResize: function () {\r\n      this.height = window.innerHeight;\r\n      this.width = window.innerWidth;\r\n      const toolbarHeight = 48;\r\n      this.cardHeight = this.height - toolbarHeight;\r\n      this.cardWidth = this.width * 0.98;\r\n    },\r\n    onClickHomePage: function (homepage) {\r\n      window.open(homepage);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAiEA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,MAAK,MAAO,yBAAyB;AAE5C,eAAe;EACbC,UAAU,EAAE;IACVF,SAAS;IACTC;EACF,CAAC;EACDE,KAAK,EAAE;IACL;EAAA,CACD;EACDC,IAAI,EAAEA,CAAA,MAAO;IACXC,WAAW,EAAE,IAAI;IAEjBC,KAAK,EAAE,CACL;MACEC,KAAK,EAAE,eAAe;MACtBC,IAAI,EAAE,UAAU;MAChBC,KAAK,EAAE,WAAW;MAClBC,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAE;IACX,CAAC,EACD;MACEJ,KAAK,EAAE,kBAAkB;MACzBC,IAAI,EAAE,kBAAkB;MACxBC,KAAK,EAAE,eAAe;MACtBC,QAAQ,EAAE,KAAK;MACfC,OAAO,EACL;IACJ,CAAC,EACD;MACEJ,KAAK,EAAE,iBAAiB;MACxBC,IAAI,EAAE,gBAAgB;MACtBC,KAAK,EAAE,2BAA2B;MAClCC,QAAQ,EAAE,QAAQ;MAClBC,OAAO,EAAE,kBAAkB;MAC3BC,QAAQ,EAAE;IACZ,CAAC,EACD;MACEL,KAAK,EAAE,kBAAkB;MACzBC,IAAI,EAAE,mBAAmB;MACzBC,KAAK,EAAE,+BAA+B;MACtCE,OAAO,EAAE;IACX,CAAC,CACF;IACDE,MAAM,EAAEC,MAAM,CAACC,WAAW;IAC1BC,KAAK,EAAEF,MAAM,CAACG,UAAU;IACxBC,UAAU,EAAE,CAAC;IACbC,SAAS,EAAE;EACb,CAAC,CAAC;EACFC,OAAO,EAAE,SAAAA,CAAA,EAAY;IACnB,IAAI,CAACC,QAAQ,CAAC,CAAC;EACjB,CAAC;EACDC,OAAO,EAAE;IACPD,QAAQ,EAAE,SAAAA,CAAA,EAAY;MACpB,IAAI,CAACR,MAAK,GAAIC,MAAM,CAACC,WAAW;MAChC,IAAI,CAACC,KAAI,GAAIF,MAAM,CAACG,UAAU;MAC9B,MAAMM,aAAY,GAAI,EAAE;MACxB,IAAI,CAACL,UAAS,GAAI,IAAI,CAACL,MAAK,GAAIU,aAAa;MAC7C,IAAI,CAACJ,SAAQ,GAAI,IAAI,CAACH,KAAI,GAAI,IAAI;IACpC,CAAC;IACDQ,eAAe,EAAE,SAAAA,CAAUZ,QAAQ,EAAE;MACnCE,MAAM,CAACW,IAAI,CAACb,QAAQ,CAAC;IACvB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}