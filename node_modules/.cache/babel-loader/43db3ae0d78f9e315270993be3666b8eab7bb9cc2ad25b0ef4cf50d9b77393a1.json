{"ast":null,"code":"import MtToolbar from \"@/components/MtToolbar.vue\";\nimport MtCard from \"@/components/MtCard.vue\";\nexport default {\n  components: {\n    MtToolbar,\n    MtCard\n  },\n  props: {\n    //\n  },\n  data: () => ({\n    initialized: true,\n    items: [{\n      color: \"red-lighten-2\",\n      icon: \"mdi-star\",\n      title: \"11:00 LAX\",\n      subtitle: \"到着\",\n      comment: \"\"\n    }, {\n      color: \"purple-lighten-2\",\n      icon: \"mdi-book-variant\",\n      title: \"13:00 Walmart\",\n      subtitle: \"買い物\",\n      comment: \"超大型スーパー。LAX空港から車で約20分。\\r\\n入国手続きなどが早めに終わった場合、水やおやつなどの買い出し\\r\\n→もし手続きに時間がかかったりしたら、ホテルチェックアウト後に行ってもいい\"\n    }, {\n      color: \"green-lighten-1\",\n      icon: \"mdi-airballoon\",\n      title: \"15:00 Miyako Hybrid Hotel\",\n      subtitle: \"チェックイン\",\n      comment: \"Walmartから車で約10分。\",\n      homepage: \"https://www.miyakohybridhotel.com/miyako-jp/\"\n    }, {\n      color: \"indigo-lighten-2\",\n      icon: \"mdi-layers-triple\",\n      title: \"19:00 - 20:00 In-N-Out Burger\",\n      comment: \"ホテルから車で約10分。ポテトはwell-doneで頼もうね。\"\n    }],\n    height: window.innerHeight,\n    width: window.innerWidth,\n    cardHeight: 0,\n    cardWidth: 0\n  }),\n  mounted: function () {\n    this.onResize();\n  },\n  methods: {\n    onResize: function () {\n      this.height = window.innerHeight;\n      this.width = window.innerWidth;\n      const toolbarHeight = 48;\n      this.cardHeight = this.height - toolbarHeight;\n      this.cardWidth = this.width * 0.98;\n    },\n    onClickHomePage: function (homepage) {\n      window.open(homepage);\n    }\n  }\n};","map":{"version":3,"names":["MtToolbar","MtCard","components","props","data","initialized","items","color","icon","title","subtitle","comment","homepage","height","window","innerHeight","width","innerWidth","cardHeight","cardWidth","mounted","onResize","methods","toolbarHeight","onClickHomePage","open"],"sources":["C:\\work\\my-trip\\src\\views\\MtSchedulePage.vue"],"sourcesContent":["<!-- MtSchedulePage.vue -->\r\n<template>\r\n  <div style=\"height: 100%; width: 100%; overflow: hidden\">\r\n    <div\r\n      v-if=\"initialized\"\r\n      v-resize=\"onResize\"\r\n      style=\"height: 100%; width: 100%; overflow: hidden\"\r\n    >\r\n      <v-main style=\"background-color: #eeeeee\">\r\n        <v-container fluid>\r\n          <transition name=\"12r\" mode=\"ont-in\">\r\n            <MtCard color=\"#eeeeee\" :height=\"height\" :width=\"width\">\r\n              <MtCard :height=\"cardHeight\" :width=\"cardWidth\">\r\n                <MtToolbar title=\"Schedule\" />\r\n                <MtCard\r\n                  style=\"overflow: auto; margin: 16px\"\r\n                  height=\"800\"\r\n                  width=\"800\"\r\n                >\r\n                  <v-timeline side=\"end\" line-inset=\"12\">\r\n                    <v-timeline-item\r\n                      v-for=\"(item, index) in items\"\r\n                      :key=\"index\"\r\n                      :width=\"500\"\r\n                      :dot-color=\"item.color\"\r\n                      :icon=\"item.icon\"\r\n                      fill-dot\r\n                    >\r\n                      <v-card>\r\n                        <v-card-title :class=\"['text-h6', `bg-${item.color}`]\">\r\n                          {{ item.title }}\r\n                        </v-card-title>\r\n                        <v-card-subtitle\r\n                          :class=\"['text-caption', `bg-${item.color}`]\"\r\n                        >\r\n                          {{ item.subtitle }}</v-card-subtitle\r\n                        >\r\n                        <v-card-text class=\"text-left\">\r\n                          {{ item.comment }}\r\n                        </v-card-text>\r\n                        <div class=\"d-flex justify-start\">\r\n                          <v-btn\r\n                            outline=\"false\"\r\n                            @click=\"onClickHomePage