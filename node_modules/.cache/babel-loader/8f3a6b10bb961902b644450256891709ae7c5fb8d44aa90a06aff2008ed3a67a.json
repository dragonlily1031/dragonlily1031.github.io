div v-resize=\"onResize\">\r\n    <MtCard color=\"#eeeeee\" :height=\"height\" :width=\"width\">\r\n      <MtCard class=\"overflow: auto\" :height=\"height\" :width=\"width\">\r\n        <MtToolbar title=\"Schedule\"></MtToolbar>\r\n        <v-timeline side=\"end\" line-inset=\"12\">\r\n          <v-timeline-item\r\n            v-for=\"(item, index) in items\"\r\n            :key=\"index\"\r\n            :width=\"500\"\r\n            :dot-color=\"item.color\"\r\n            :icon=\"item.icon\"\r\n            fill-dot\r\n          >\r\n            <v-card>\r\n              <v-card-title :class=\"['text-h6', `bg-${item.color}`]\">\r\n                {{ item.title }}\r\n              </v-card-title>\r\n              <v-card-subtitle :class=\"['text-caption', `bg-${item.color}`]\">\r\n                {{ item.subtitle }}</v-card-subtitle\r\n              >\r\n              <v-card-text class=\"text-left\">\r\n                {{ item.comment }}\r\n              </v-card-text>\r\n              <div class=\"d-flex justify-start\">\r\n                <v-btn outline=\"false\" @click=\"onClickHomePage(item.homepage)\">\r\n                  HOMEPAGE\r\n                </v-btn>\r\n                <v-btn outline=\"false\" :to=\"item.map\"> Google MAP </v-btn>\r\n              </div>\r\n            </v-card>\r\n          </v-timeline-item>\r\n        </v-timeline>\r\n      </MtCard>\r\n    </MtCard>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MtToolbar from \"@/components/MtToolbar.vue\";\r\nimport MtCard from \"@/components/MtCard.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    MtToolbar,\r\n    MtCard,\r\n  },\r\n  props: {\r\n    //\r\n  },\r\n  data: () => ({\r\n    items: [\r\n      {\r\n        color: \"red-lighten-2\",\r\n        icon: \"mdi-star\",\r\n        title: \"11:00 LAX\",\r\n        subtitle: \"到着\",\r\n        comment: \"\",\r\n      },\r\n      {\r\n        color: \"purple-lighten-2\",\r\n        icon: \"mdi-book-variant\",\r\n        title: \"