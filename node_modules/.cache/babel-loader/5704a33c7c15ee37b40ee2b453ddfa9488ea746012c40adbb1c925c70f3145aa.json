{"ast":null,"code":"import MtToolbar from \"@/components/MtToolbar.vue\";\nimport MtCard from \"@/components/MtCard.vue\";\nimport MtDialog from \"@/dialog/MtDialog.vue\";\nexport default {\n  components: {\n    MtToolbar,\n    MtCard,\n    MtDialog\n  },\n  props: {\n    //\n  },\n  data: () => ({\n    height: 0,\n    width: 0,\n    pageHeight: 0,\n    pageWidth: 0,\n    selectIndex: null\n  }),\n  mounted: function () {\n    this.onResize();\n  },\n  computed: {\n    //\n  },\n  methods: {\n    onResize: function () {\n      this.height = window.innerHeight;\n      this.width = window.innerWidth;\n      const margin = 12;\n      this.pageHeight = window.innerHeight - margin;\n      this.pageWidth = window.innerWidth - margin;\n      const headerHeight = 64;\n      this.tabsAndTimelineHeight = this.pageHeight - headerHeight;\n    }\n  }\n};","map":{"version":3,"names":["MtToolbar","MtCard","MtDialog","components","props","data","height","width","pageHeight","pageWidth","selectIndex","mounted","onResize","computed","methods","window","innerHeight","innerWidth","margin","headerHeight","tabsAndTimelineHeight"],"sources":["C:\\work\\my-trip\\src\\pages\\MtEmergencyPage.vue"],"sourcesContent":["<!-- MtEmergencyPage.vue -->\r\n<template>\r\n  <div v-resize=\"onResize\" style=\"height: 100%; width: 100%; overflow: hidden\">\r\n    <!-- 下地 グレー -->\r\n    <MtCard\r\n      flat\r\n      color=\"#999999\"\r\n      style=\"overflow: hidden\"\r\n      :height=\"height\"\r\n      :width=\"width\"\r\n    >\r\n      <!-- 下地 ホワイト -->\r\n      <MtCard\r\n        style=\"margin: 6px; overflow: hidden\"\r\n        :height=\"pageHeight\"\r\n        :width=\"pageWidth\"\r\n      >\r\n        <!-- 緊急時メモ部分 -->\r\n        <MtToolbar title=\"Emergency\" :elevation=\"6\"></MtToolbar>\r\n        <v-tabs v-model=\"selectIndex\" color=\"primary\">\r\n          <v-tab v-for=\"tip in tips\" :key=\"tip.title\" :prepend-icon=\"tip.icon\">\r\n            {{ tip.title }}</v-tab\r\n          >\r\n    